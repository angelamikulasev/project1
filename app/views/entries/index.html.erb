<h3>Entries</h3>

<% if @entries.present? %>
 <ul>
	<% @entries.each do |entry| %>
	  <li><%= entry.country.country %></li>
	  <li><%= link_to(entry.name, entry)%></li>
	<% end %>
 </ul>
<% else %>
	<p>There are no entries to display.</p>
<% end %>